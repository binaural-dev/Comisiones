<odoo>
    <data>
        <!--heredar formulario de pickigns-->
        <record id="view_picking_form_inherited_binaural_marca" model="ir.ui.view">
            <field name="name">stock.picking.form.binaural</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='description_picking']" position="after">
                    <field name="brand_id"/>
                </xpath> 
            </field>
        </record>
    </data>
</odoo>