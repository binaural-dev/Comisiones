<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="employee_inherit_binaural_nomina_reportes_form" model="ir.ui.view">
        <field name="name">binaural.nomina.reportes.employee.form</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr.view_employee_form"/>
        <field name="arch" type="xml">
            <notebook position="inside">
                <page string="IVSS Configuration">
                    <group>
                        <group>
                            <field name="entry_occupation_id" required="1"/>
                            <field name="motor_skill" requited="1"/>
                        </group>
                        <group>
                            <field name="type" widget="selection" required="1"/>
                            <field name="condition" required="1"/>
                        </group>
                    </group>
                </page>
            </notebook>
        </field>
    </record>

</odoo>
