<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record id="binaural_vendedores_inventario_inherit_vpick_tree" model="ir.ui.view">
        <field name="name">binaural.rvendedores.inventario.inherit.vpick.tree</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.vpicktree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="seller_id" optional="show"/>
            </xpath>
        </field>
    </record>

    <record id="binaural_inventario_vendedores_inherit_view_form" model="ir.ui.view">
        <field name="name">binaural.inventario.vendedores.inherit.view.form</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="seller_id"/>
            </xpath>
        </field>
    </record>

</odoo>
