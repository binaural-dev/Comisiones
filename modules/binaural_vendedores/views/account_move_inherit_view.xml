<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="binaural_vendedores_account_move_inherit_form_view" model="ir.ui.view">
        <field name="name">binaural.vendedores.account.move.inherit.form.view</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='invoice_user_id']" position="inside">
                <field name="seller_id" />
            </xpath>
        </field>
    </record>

</odoo>